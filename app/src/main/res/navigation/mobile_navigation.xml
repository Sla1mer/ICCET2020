<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.iccet2020.ui.home.HomeFragment"
        android:label="Личные данные"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.example.iccet2020.ui.TakeNote.TakeNoteFragment"
        android:label="Запись на прием"
        tools:layout="@layout/fragment_take_note" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.example.iccet2020.ui.History.HistoryFragment"
        android:label="История"
        tools:layout="@layout/fragment_notifications" />
</navigation>